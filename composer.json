{
    "name": "bnomei/kirby3-security-headers",
    "type": "kirby-plugin",
    "version": "1.1.3",
    "license": "MIT",
    "description": "Kirby 3 Plugin for easier Security Headers setup",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "content-security-policy",
        "security-headers",
        "csp"
    ],
    "autoload": {
        "psr-4": {
            "Bnomei\\": "classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "martijnc/php-csp": "^1.0",
        "getkirby/composer-installer": "^1.1"
    }
}
